{
	"name": "@dming/pinus-scheduler",
	"version": "1.4.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/dming/pinus.git"
	},
	"publishConfig": {
	"registry": "https://npm.pkg.github.com/dming"
	},
	"author": "mybios <mybios@qq.com>",
	"main": "./dist/schedule",
	"types": "./lib/schedule",
	"files": [
		"dist",
		"lib",
		"test"
	],
	"scripts": {
		"build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
		"fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-scheduler lib/"
	},
	"engines": {
		"node": ">=8.5"
	},
	"dependencies": {
		"@types/node": "8.10.54",
		"pinus-logger": "npm:@dming/pinus-logger@^1.4.2"
	},
	"devDependencies": {
		"tslint": "5.20.1",
		"typescript": "3.7.3"
	}
}
